{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "no-console": "off",
        "no-underscore-dangle":  ["error", { "allow": [
            "_service",
            "_notes",
            "_validator",
            "_pool",
            "_authenticationsService",
            "_usersService",
            "_tokenManager",
            "_collaborationService",
            "_collaborationsService",
            "_notesService",
            "_exports",
            "_folder",
            "_data",
            "_S3"
        ]}]
    }
}
